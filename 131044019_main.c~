#include<stdio.h>
#include<string.h>
int main(int argc, char* argv[]){
    char *tiffFile;
    
    if(argc != 2){
        fprintf(stdout, "Usage >>\n");
        fprintf(stdout, "./tiffProcessor testFile.tiff\n");
    }

    strcpy(tiffFile, argv[1]);
    
    printf("tiffFile: %s\n", argv[1]);

    return 0;
}
